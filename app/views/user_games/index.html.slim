/ - model_class = UserGame

/ .page-header
/   h1=t '.title', :default => model_class.model_name.human.pluralize.titleize

/ / = render partial: 'filter'
/ table.table.table-striped
/   thead
/     tr
/       th= model_class.human_attribute_name(:unique_session_id)
/       th= model_class.human_attribute_name(:name)
/       th= model_class.human_attribute_name(:created_at)
/       th=t '.actions', :default => t("helpers.actions")
/   tbody
/     - @user_games.each do |user_game|
/       tr
/         td= link_to user_game.unique_session_id, user_game_path(user_game)
/         td= user_game.name
/         td=l user_game.created_at
/         td
/           = link_to t('.show', :default => t("helpers.links.show")), user_game_path(user_game), :class => 'btn btn-default btn-xs'
/           / '
/           / = link_to t('.edit', :default => t("helpers.links.edit")), edit_user_game_path(user_game), :class => 'btn btn-default btn-xs'
/           / '
/           / = link_to t('.destroy', :default => t("helpers.links.destroy")), user_game_path(user_game), :method => :delete, :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) }, :class => 'btn btn-xs btn-danger'

/ / = link_to t('.new', :default => t("helpers.links.new")), new_user_game_path, :class => 'btn btn-primary'

#app_vue

== javascript_pack_tag 'application'
